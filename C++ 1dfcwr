#include<iostream>
using namespace std;

int main() {
	for (int i = 0; i < 6; i++) { //outer loop: rows
		for (int j = 0; j < 10; j++) {//inner loop :columns
			cout << "$";
		}
		cout << endl;
	}
	cout << " " << endl;

	for (int i = 0; i < 1; i++) { //rows
		for (int j = 0; j < 6; j++) { //columns
			cout << j << "";
		}//end of inner loop
		cout << endl; //inside outer loop but no inner loop!
	}

	cout << endl;

	for (int i = 0; i < 1; i++) { //rows
		for (int j = 0; j < 6; j++) { //columns
			cout << j << "";
		}//end of inner loop
		cout << endl; //inside outer loop but no inner loop!
	}

	cout << endl;

	for (int i = 0; i < 1; i++) { //rows
		for (int j = 0; j < 6; j++) { //columns
			cout << j << "";
		}//end of inner loop
		cout << endl; //inside outer loop but no inner loop!
	}

	cout << endl;



}
